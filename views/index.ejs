<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <% messages.forEach(item => { %>
      <ul>
        <li><%= item.text %></li>
        <li><%= item.user %></li>
        <li><%= item.added %></li>
      </ul>
    <% }) %> 
  </body>
</html>
